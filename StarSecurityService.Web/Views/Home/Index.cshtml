                                                <div class="desc_wrapper title-wrapper">
                                                    <h5 class="title" title="@item.Name">@item.Name</h5>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                }
                            </div>
                        </div>
                    </div>
                </div>
                <div class="section mcb-section mcb-section-5ydhpqene  no-margin-v bt-top" style="padding-bottom:10px;background-color:#ffffff">
                    <div class="section_wrapper mcb-section-inner">
                        <div class="wrap mcb-wrap mcb-wrap-c42dxzzzt one-second  valign-middle clearfix" style="">
                            <div class="mcb-wrap-inner">
                                <div class="column mcb-column mcb-item-g5khn0yb7 one column_image" style="padding-top: 30px">
                                    <div class="animate zoomIn" data-anim-type="zoomIn">
                                        <div class="image_frame image_item no_link scale-with-grid aligncenter no_border">
                                            <div class="image_wrapper">
                                                @if (!string.IsNullOrEmpty(Model?.HistoryModel?.Image))
                                                {
                                                    <img class="scale-with-grid"
                                                         src="../../Areas/Asset/img/@(Model?.HistoryModel?.Image)" alt=""
