@model StarSecurityService.Web.Models.HomeIndexModel
@{
    ViewBag.Title = "Star Security";
}

@section style{
    <style>
        .column_icon_box .title-wrapper {
            position: absolute;
            top: 60px;
            left: 17px;
            right: 0px;
        }

        .style-simple .icon_box .title-wrapper .title {
            color: white;
            display: -webkit-box;
            -webkit-line-clamp: 3;
            -webkit-box-orient: vertical;
            max-width: 160px;
            overflow: hidden;
            text-overflow: ellipsis;
        }
    </style>
}
<div id="Content">
    <div class="content_wrapper clearfix">

        <div class="sections_group">

            <div class="entry-content" itemprop="mainContentOfPage">

                <div class="section mcb-section mcb-section-ylkmrierc" style="padding-top:80px;padding-bottom:10px;background-color:#ffffff">
                    <div class="section_wrapper mcb-section-inner">
                        <div class="wrap mcb-wrap mcb-wrap-187q43x0j one  valign-top clearfix" style="">
                            <div class="mcb-wrap-inner">
                                <div class="column mcb-column mcb-item-lh3o7uyfp one-fifth column_column">
                                    <div class="column_attr clearfix align_left animate fadeInLeft" data-anim-type="fadeInLeft" style="padding:90px 0 0;">
                                        <h3 style="color:#6d6e72">OUR<br>SERVICES</h3>
                                    </div>
                                </div>
                                @foreach (var item in Model.CategoryServiceOfferModel)
                                {
