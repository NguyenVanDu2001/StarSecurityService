                                    <div class="column_attr clearfix align_left animate fadeInRight" data-anim-type="fadeInRight" style="padding:20px 0 0;">
                                        <h3 style="color:#6d6e72">@(Model?.HistoryModel?.Title)</h3>
                                        <p>
                                            @(Model?.HistoryModel?.Description)
